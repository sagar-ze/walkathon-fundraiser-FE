import axios from 'axios';

export const RegisterService = {
    async getWevents() {
        const res = await axios.get('https://jsonplaceholder.typicode.com/users')
        const data = res.data
    
        const wevents = data.map(d => ({
          "value" : d.id,
          "label" : d.name
        }))

        this.setState({selectOptions: wevents})
    }
}